{
  "version": "1.0.0",
  "metadata": {
    "description": "Multi-MCP server configuration for Talk2Tables",
    "created": "2025-01-20T10:00:00Z",
    "author": "admin@talk2tables.com"
  },
  "defaults": {
    "timeout": 30000,
    "retry_attempts": 3,
    "retry_delay": 1000
  },
  "servers": [
    {
      "name": "database-server",
      "enabled": true,
      "description": "SQLite database MCP server",
      "transport": "sse",
      "priority": 100,
      "critical": true,
      "config": {
        "url": "http://localhost:8000/sse",
        "headers": {}
      }
    },
    {
      "name": "fetch-server",
      "enabled": true,
      "description": "Fetch MCP server for web content retrieval",
      "transport": "stdio",
      "priority": 50,
      "critical": false,
      "config": {
        "command": "uvx",
        "args": ["mcp-server-fetch"],
        "env": {}
      }
    }
  ]
}